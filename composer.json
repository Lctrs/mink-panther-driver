{
  "name": "lctrs/mink-panther-driver",
  "type": "library",
  "description": "Panther (WebDriver) driver for Mink framework",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "behat/behat": "^3.6.1",
    "behat/mink": "^1.7.1",
    "behat/mink-extension": "^2.3.1",
    "php-webdriver/webdriver": "^1.8.1",
    "symfony/config": "~3.4.37 || ~4.3.11 || ~4.4.4",
    "symfony/dependency-injection": "~3.4.37 || ~4.3.11 || ~4.4.4",
    "symfony/panther": "^0.6.1",
    "webmozart/assert": "^1.7.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^7.0.2",
    "ergebnis/composer-normalize": "^2.2.3",
    "maglnet/composer-require-checker": "^2.0.0",
    "matthiasnoback/symfony-config-test": "^4.1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^3.1.0",
    "mink/driver-testsuite": "dev-master",
    "ondram/ci-detector": "^3.2.0",
    "phpstan/extension-installer": "^1.0.3",
    "phpstan/phpstan": "^0.12.11",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpstan/phpstan-webmozart-assert": "^0.12.2",
    "phpunit/phpunit": "^7.5.20",
    "psalm/plugin-phpunit": "^0.9.0",
    "vimeo/psalm": "^3.9.3"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Lctrs\\MinkPantherDriver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lctrs\\MinkPantherDriver\\Test\\": "test/"
    }
  }
}
