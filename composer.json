{
    "name": "lctrs/mink-panther-driver",
    "type": "library",
    "description": "Panther (WebDriver) driver for Mink framework",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "facebook/webdriver": "^1.6",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/panther": "^0.2.0 || ^0.3.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "localheinz/composer-normalize": "^1.1",
        "maglnet/composer-require-checker": "^2.0",
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.1",
        "mink/driver-testsuite": "dev-master",
        "phpstan/phpstan": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Lctrs\\MinkPantherDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lctrs\\MinkPantherDriver\\Tests\\": "tests/"
        }
    }
}
